<h2>Student Table</h2>

<table class="table table-bordered">
    <thead>
        <tr>
            <th>Full Name</th>
            <th>Gender</th>
            <th>Course</th>
            <th>Section</th>
            <th>Birthday</th>
            <th>Age</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var student in students)
        {
            <tr>
                <td>@student.FullName</td>
                <td>@student.Gender</td>
                <td>@student.Course</td>
                <td>@student.Section</td>
                <td>@student.Birthday.ToString("yyyy-MM-dd")</td>
                <td>@student.Age</td>
            </tr>
        }
    </tbody>
</table>